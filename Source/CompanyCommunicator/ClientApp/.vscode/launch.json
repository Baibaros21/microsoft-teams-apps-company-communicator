{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Teams App",
        "type": "chrome",
        "request": "launch",
        "url": "https://teams.microsoft.com",
        "webRoot": "${workspaceFolder}/src",
        "sourceMaps": true,
        "skipFiles": ["node_modules/**"],
        "runtimeArgs": [
          "--remote-debugging-port=9222"
        ]
      }
    ]
  }